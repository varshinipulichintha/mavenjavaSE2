FROM eclipse-temurin:17-jdk
WORKDIR /app
COPY target/*.jar app.jar
EXPOSE 8080
ENTRYPOINT ["java", "-jar", "app.jar"]

docker run -p 8080:8080 my-java-app

FROM tomcat:9-jdk11
WORKDIR /usr/local/tomcat/webapps/
RUN rm -rf ROOT
COPY target/*.war .
EXPOSE 8080
CMD ["catalina.sh", "run"]
